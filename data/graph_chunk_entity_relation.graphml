  <data key="d2">chunk-307fbbaf9cc5087b5dba49fab909abc3</data>
</node>
<node id="&quot;SYN-FIN PACKET RESPONSE ATTACK &quot;">
  <data key="d0">"ATTACK MODE"</data>
