  <data key="d2">chunk-1e9cca2bda47c6a3436bbe670681bced</data>
</node>
<node id="&quot;EMBEDDED OPERATING SYSTEM VULNERABILITY&quot;">
  <data key="d0">"HIDDEN DANGERS"</data>
